package chap6;

public class Person_ex {
	public static void main(String[] args) {
		Person p = new Person();
		p.age = 26;
		p.name = "java";
		System.out.println(p.info());
		
		Person p2 = new Person();
		p2.age = 25;
		p2.name = "python";
		System.out.println(p2.info());
		
		Person p3 = new Person("c", 20);
		System.out.println(p3.info());
	}
}