interface Clickable {
	void iclick();
}

class Button {
	Clickable c;
	public void click() {
		System.out.println("눌러짐");
		if (c!=null) {
			c.iclick();
		}
	}
	public void addClickListenr(Clickable c) {
		this.c=c;
	}
}

class LoginButton extends Button {
	@Override
	public void click() {
		super.click();
		System.out.println("로그인 처리");
	}
}

class LoginClickable implements Clickable {
	@Override
	public void iclick() {
		System.out.println("로그인 처리");
	}
}

public class InterfaceSample2 {

	public static void main(String[] args) {
		Button btn = new LoginButton();
		btn.addClickListenr(new LoginClickable());
		btn.click();
	}
}
