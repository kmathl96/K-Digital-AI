package shapes;

public class Circle implements Shape {
	Point center;
	int radius;

	public Circle(String color, Point center, int radius) {
		super();
		this.center = center;
		this.radius = radius;
	}
	public Circle(String color, int x, int y, int radius) {
		super();
		this.center = new Point(x,y);
		this.radius = radius;
	}

	@Override
	public void draw() {
		System.out.println("[Circle-color:"+color+",center:("10","10"),radius:"5"]");
	}
}