package sec01.val;

class InitTest {
	int iv=1;
	static int[] sarr = new int[10];
	
	{ // instance 초기화 block
		System.out.println(iv);
		iv = 2;
		System.out.println(iv);
	}
	static { // static 초기화 block
		
	}
	InitTest() {
		iv = 3;
		System.out.println(iv);
	}
}

public class InitBlockTest {

	public static void main(String[] args) {
		InitTest it = new InitTest();
	}
}