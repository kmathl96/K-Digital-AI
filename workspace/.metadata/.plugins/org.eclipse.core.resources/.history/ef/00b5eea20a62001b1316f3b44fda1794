package shapes;

public class Triangle extends Shape {
	Point[] pos = new Point[3];
	
	public Triangle() {}
	public Triangle(String color, Point x, Point y, Point z) {
		super(color);
		pos[0] = x;
		pos[1] = y;
		pos[2] = z;
	}

	@Override
	public void draw() {
		// TODO Auto-generated method stub

	}

}
