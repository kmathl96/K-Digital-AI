package chap5;

public class CopyArySample {

	public static void main(String[] args) {
		int[] ary = new int[] {10,20,30};
		int[] cpy = new int[3];
		
		cpy = ary;
		ary[0] = 100;
		for (int i=0; i<ary.length; i++) {
			System.out.println(ary[i]);
		}
		System.out.println();
		for (int i=0; i<cpy.length; i++) {
			System.out.println(cpy[i]);
		}
	}
}