package emp;

public abstract class Employee {
	String id;
	String name;
	
	public Employee(String id, String name) {
		this.id = id;
		this.name = name;
	}
	
	
	public int getPay() {
		return 0;
	}
//	public void setPay(int pay) {
//		this.pay = pay;
//	}

	public String info() {
		return "사번:" + id + ", 이름:" + name;
	}
	
}