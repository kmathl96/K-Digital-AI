package chap6;

class Product {
	int serialNum = 0;
	static int snum = 100;
	public Product() {
		serialNum++;
		snum++;
		System.out.println("serialNum:"+serialNum+", snum:"+snum);
	}
}

public class StaticTest {
	public static void main(String[] args) {
		Product[] prs = new Product[5];
		for (int i=0; i<prs.length; i++) {
			prs[i] = new Product();
		}
	}
}
